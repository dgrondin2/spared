<div class='org-dash-nav left-dash-nav'>
  <h2>TAKE ACTION:</h2>

  <% if current_page?(org_my_wishlists_path) %>
      <%= link_to image_tag('view-edit-wishlists-selected.png'), org_my_wishlists_path, class: 'active' %>
  <% else %>
      <%= link_to image_tag('view-edit-wishlists-dashnav.png'), org_my_wishlists_path %>
  <% end %>

  <% if current_page?(org_distribute_donations_path) %>
      <%= link_to image_tag('distribute-funds-selected.png'), org_distribute_donations_path, class: 'active' %>
  <% else %>
      <%= link_to image_tag('distribute-funds-dashnav.png'), org_distribute_donations_path %>
  <% end %>

  <% if current_page?(org_manage_events_path) %>
      <%= link_to image_tag('manage-volunteers-selected.png'), org_manage_events_path, class: 'active' %>
  <% else %>
      <%= link_to image_tag('manage-volunteers-dashnav.png'), org_manage_events_path %>
  <% end %>
</div>

<div class='org-dash-nav right-dash-nav'>
  <h2>BROWSE:</h2>

  <% if current_page?(browse_events_path) %>
      <%= link_to image_tag('volunteer-opportunities-org-selected.png'), browse_events_path, class: 'active' %>
  <% else %>
      <%= link_to image_tag('donor-navigation/standardcropped.png'), browse_events_path %>
  <% end %>

  <% if current_page?(browse_wishlists_path) %>
      <%= link_to image_tag('organization-wishlists-selected.png'), browse_wishlists_path, class: 'active' %>
  <% else %>
      <%= link_to image_tag('donor-navigation/standard1cropped.png'), browse_wishlists_path %>
  <% end %>

  <% if current_page?(browse_item_offers_path) %>
      <%= link_to image_tag('item-offer-board-org-selected.png'), browse_item_offers_path, class: 'active' %>
  <% else %>
      <%= link_to image_tag('donor-navigation/standard2cropped.png'), browse_item_offers_path %>
  <% end %>
</div>